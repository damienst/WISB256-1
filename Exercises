>>> height = 12.0
>>> delimiter = '.'
>>> width/2
8.5
>>> type(width)
<class 'int'>
>>> type(width/2)
<class 'float'>
>>> width/2.0
8.5
>>> type(width/2.0)
<class 'float'>
>>> height/3
4.0
>>> type(height/3)
<class 'float'>
>>> 1+2*5
11
>>> type(1+2*5)
<class 'int'>
>>> delimiter*5
'.....'
>>> type(delimiter*5)
<class 'str'>

